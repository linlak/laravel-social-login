{
    "name": "linlak/laravel-social-login",
    "minimum-stability": "dev",
    "description": "Laravel Social plugin built on to of socialite Library with api support",
    "homepage": "https://github.com/linlak/laravel-social-login",
    "support": {
        "issues": "https://github.com/linlak/laravel-social-login/issues",
        "source": "https://github.com/linlak/laravel-social-login"
    },
    "license": "MIT",
    "keywords": [
        "laravel-login",
        "laravel social login",
        "social login",
        "facebook login",
        "laravel socialite"
    ],
    "authors": [{
        "name": "Linus Nowomukama",
        "email": "nowomukama.linlak.linus2@gmail.com",
        "role": "Developer"
    }],
    "type": "library",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "5.8.*",
        "illuminate/http": "5.8.*",
        "illuminate/contracts": "5.8.*",
        "illuminate/auth": "5.8.*",
        "laravel/socialite": "^4.1.3"
    },
    "require-dev": {
        "illuminate/console": "5.8.*"
    },
    "autoload": {
        "psr-4": {
            "Linlak\\LinSocial\\": "src/"
        }
    },
    "suggest": {
        "ramsey/uuid": "Required to perform string manulaption such as genarating RFC 4122 version 1,3,4 and 5 univasary unique identifiers (UUID)."
    },
    "extra": {
        "laravel": {
            "providers": [
                "Linlak\\LinSocial\\Providers\\LinSocialProvider"
            ],
            "aliases": {
                "LinJwt": "Linlak\\LinSocial\\Facades\\LinSocial"
            }
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
